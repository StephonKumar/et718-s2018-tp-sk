<h1>Boatcomments</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Comment</th>
      <th>Boat</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @boatcomments.each do |boatcomment| %>
      <tr>
        <td><%= boatcomment.user %></td>
        <td><%= boatcomment.comment %></td>
        <td><%= boatcomment.boat %></td>
        <td><%= link_to 'Show', boatcomment %></td>
        <td><%= link_to 'Edit', edit_boatcomment_path(boatcomment) %></td>
        <td><%= link_to 'Destroy', boatcomment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

